{
    "name": "laratrade/gdax",
    "type": "library",
    "description": "Laravel package for GDAX integration.",
    "keywords": [
        "laravel",
        "laratrade",
        "gdax"
    ],
    "homepage": "https://github.com/laratrade/gdax",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgenii Nasyrov",
            "email": "inasyrov@ya.ru",
            "homepage": "https://github.com/nasyrov",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.5.*|5.6.*",
        "ratchet/pawl": "~0.3"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5|~3.6",
        "phpunit/phpunit": "~6.0|~7.0",
        "squizlabs/php_codesniffer": "~3.2"
    },
    "autoload": {
        "psr-4": {
            "Laratrade\\GDAX\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laratrade\\GDAX\\Tests\\": "tests"
        }
    },
    "scripts": {
        "lint": "phpcs -p --standard=psr2 src",
        "phpunit": "phpunit",
        "phpunit-integration": "phpunit --testsuite Integration",
        "phpunit-unit": "phpunit --testsuite Unit",
        "phpunit-coverage": "phpunit --coverage-clover=coverage.clover",
        "test": [
            "@lint",
            "@phpunit-coverage"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laratrade\\GDAX\\GDAXServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
